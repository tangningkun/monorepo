{
    "extends": "@setting/tsconfig/base.json",
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "jsx": "preserve",
        "moduleResolution": "node",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "#/*": [
                "src/*"
            ]
        },
        "lib": [
            "dom",
            "esnext"
        ],
        "types": [
            "vite/client",
        ]
    },
    "include": [
        "src",
        "mock",
        "vite.config.ts",
    ]
}